generator client {  provider      = "prisma-client-js"  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]}datasource db {  provider = "postgresql"  url      = env("DATABASE_URL_PRISMA")}model Employee {  id        String @id @default(cuid())  email     String @unique  password  String  firstName String  lastName  String  session Session[]}enum SessionStatus {  available  reserved  finished}model Session {  id            String        @id @default(cuid())  theme         String  duration      Int  startDate     DateTime  endDate       DateTime  price         Int  minimalClient Int  status        SessionStatus  employee Employee[]}